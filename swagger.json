            "summary": "Get all restaurants in system",
            "responses": {
            "200": {
                "description": "OK",
                "schema": {
                "$ref": "#/definitions/Restaurants"
                }
            }
            }
        }
        }
    },
    "definitions": {
        "Restaurant": {
            "required": ["name", "_id", "type"],
            "properties": {
                "_id": {
                "type": "integer",
                "uniqueItems": true
                },
                "name": {
                    "type": "string",
                    "example": "Pizzeria La Escala"
                },
                "type": {
                    "type": "string",
                    "example": "Italiana"
                }
            }
        },
        "Restaurants": {
        "type": "array",
        "$ref": "#/definitions/Restaurant"
        }
    }
