    "/restaurants": {
      "get": {
        "tags": [
          "Restaurant"
        ],
        "summary": "Retrieve all restaurants, can be filtered.",
        "description": "Retrieve all restaurants from the database,    can be filtered by name, price range, restaurant type, minRating and maxDistance.",
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "description": "User id, it's used to determine location and relevance according to the user",
            "required": true,
            "type": "string"
          },
          {
            "name": "name",
            "in": "query",
            "description": "Filter by name",
            "required": false,
            "type": "string"
          },
          {
            "name": "priceRange",
            "in": "query",
            "description": "Filter by price range, can be $, $$, $$$ or $$$$ ($ is the cheapest, $$$$ the most expensive). Can be comma separated for multiples.",
            "required": false,
            "type": "string"
          },
          {
            "name": "restaurantTypes",
            "in": "query",
            "description": "Filter by restaurant type, can be one or more (needs to be comma separated)",
            "required": false,
            "type": "string"
          },
          {
            "name": "minRating",
            "in": "query",
            "description": "Filter by minimum rating, can be from 1 to 5",
            "required": false,
            "type": "string"
          },
          {
            "name": "maxDistance",
            "in": "query",
            "description": "Filter by maximum distance, can be from 0 to 100 km",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/Restaurants"
            }
          },
          "500": {
            "description": "Some error occurred while retrieving restaurants."
